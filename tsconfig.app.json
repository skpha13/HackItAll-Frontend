{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue",
              "src/models/*.ts",
              "src/models/*.ts",
              "src/models/**/*.ts",
              "src/helpers/**/*.ts",
              "src/helpers/AxiosIntance.ts",
              "src/models/Generic.ts",
              "src/models/Car.ts",
              "src/models/Battery.ts",
              "src/utils/*.ts",
              "src/utils/*.ts",
              "src/utils/**/*.ts",
              "formkit.config.ts",
              "formkit.theme.ts",
  ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",

    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
